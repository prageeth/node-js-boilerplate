{
  "name": "@prageeths/node-js-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for React JS using node",
  "license": "Apache-2.0",
  "keywords": [
    "node",
    "boilerplate",
    "react"
  ],
  "main": "dist/main.js",
  "scripts": {
    "build": "babel src --out-dir ./dist --source-maps --ignore ./test",
    "test": "./node_modules/.bin/mocha --require @babel/register test/**/*test.js",
    "prepare": "npm run-script build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:prageeth/node-js-boilerplate.git"
  },
  "dependencies": {
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/plugin-proposal-class-properties": "latest",
    "@babel/plugin-proposal-object-rest-spread": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "@babel/register": "latest",
    "chai": "latest",
    "coffeescript": "latest",
    "enzyme": "latest",
    "enzyme-adapter-react-16": "latest",
    "mocha": "latest",
    "sinon": "latest",
    "sinon-chai": "latest"
  }
}
